#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include<conio.h>
int main()
{
	int i,x,not;
	char no[20],mvp[100],ca[100];
	for (i=1;i<16;i++){
		printf("\n Ingrese la nota del alumno %d ",i);
		scanf("%d",&not);
		printf("\n Ingrese el nombre del alumno %d ",i);
		scanf("%s",&no);
		if (not>=8){
			strcpy(mvp,no);
			strcat(ca,mvp);
			strcat(ca," ");		
	}
	}
	printf("\n Los mejores son ");
	printf("%s ",ca);
	return 0;
}
